<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ======== Interaction ============== -->
  <SoundDef>
    <defName>Interact_CGear</defName>
    <sustain>False</sustain>
    <context>MapOnly</context>
    <eventNames />
    <priorityMode>PrioritizeNearest</priorityMode>
    <sustainStartSound>
    </sustainStartSound>
    <sustainStopSound>
    </sustainStopSound>
    <!-- <maxSimultaneous>1</maxSimultaneous>  -->
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Interact/Interact_CGear</clipPath>
          </li>
        </grains>
        <muteWhenPaused>True</muteWhenPaused>
        <volumeRange>
          <min>50</min>
          <max>50</max>
        </volumeRange>
        <pitchRange>
          <min>1</min>
          <max>1.015</max>
        </pitchRange>
        <distRange>
          <min>10</min>
          <max>80</max>
        </distRange>
      </li>
    </subSounds>
  </SoundDef>
