<Defs>
  <RecipeDef>
    <defName>MakeCGearChip</defName>
    <label>Make chip TC Control</label>
    <description>Craft a TC control chip.</description>
    <jobString>Making a TC control chip.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <allowMixingIngredients>false</allowMixingIngredients>
    <workAmount>500</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <unfinishedThingDef>UnfinishedCGearCtrlChip</unfinishedThingDef>
    <workSkill>Crafting</workSkill>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Plasteel</li>
        <li>Silver</li>
        <li>Chemfuel</li>
        <li>ComponentSpacer</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <CGearCtrlChip>1</CGearCtrlChip>
    </products>
    <recipeUsers>
      <li>CGearChipStand</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <researchPrerequisite>CGearTech</researchPrerequisite>
  </RecipeDef>
